@{
}
@model List<Request>
<div>
    <table>
        <thead>
            <tr>
                <td> Request Id </td>
                <td> Student Name </td>
                <td> Subject</td>
                <td> Created By </td>
                <td> Due Date </td>
                <td> Status </td>
                <td>Action</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var request in Model)
            {
                <tr>
                    <td>@request.RequestId </td>
                    <td> @request.StudentName </td>
                    <td> @request.Subject</td>
                    <td> @request.CreatedDate </td>
                    <td> @request.DueDate </td>
                    <td> @request.Status </td>
                    <td>@request.Action</td>
                </tr>

            }
            
        <tbody>
    </table>
</div>

